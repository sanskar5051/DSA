class Solution {
public:
    int lengthOfLastWord(string s) {
        int ans=0;
        int n=s.length();
        int l=0;
        for(int i=0;i<n;i++){
            
            if(s[i]==' '){
                l=0;
            }
            else{
                l++;
                ans=l;
            }
        }
        return ans;
        
    }
};
